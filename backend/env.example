# Server Configuration
PORT=8000
NODE_ENV=development

# Database
DATABASE_URL=postgresql://username:password@localhost:5432/pixelperfect
REDIS_URL=redis://localhost:6379

# AI/ML Services
OPENAI_API_KEY=your_openai_api_key
GOOGLE_VISION_API_KEY=your_google_vision_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key

# File Storage
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,video/mp4,video/webm

# Agent Configuration
MAX_ITERATIONS=5
QUALITY_THRESHOLD=0.85
PIXEL_DIFF_THRESHOLD=0.05

# External APIs
UNSPLASH_ACCESS_KEY=your_unsplash_access_key
ICONIFY_API_URL=https://api.iconify.design

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/app.log

# CORS
CORS_ORIGIN=http://localhost:3000

#Environment
NODE_ENV="development"